    "description": "Схема для валидации списка типов нод",
    "type": "object",
    "properties": {
        "output": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "random": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "seed": {
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    },
    "required": [
